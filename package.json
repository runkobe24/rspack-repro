{
  "name": "zhj-web",
  "version": "2.22.0",
  "private": true,
  "scripts": {
    "dev": "rsbuild dev --mode development",
    "dev:rsdoctor": "RSDOCTOR=true rsbuild dev",
    "dev:omweb": "yarn && rsbuild dev --mode development --env-mode omweb",
    "dev:mweb": "yarn && rsbuild dev --mode development --env-mode mweb",
    "dev:web": "yarn && rsbuild dev --mode development --env-mode web",
    "build": "rsbuild build",
    "lint": "rsbuild dev lint",
    "predev": "yarn",
    "prebuild": "yarn",
    "build:sourcemap": "rsbuild build --sourcemap space",
    "upload:sourcemap": "cross-var sentry-cli releases files space@$npm_package_version upload-sourcemaps ./dist/js --url-prefix '~/js'",
    "delete:sourcemap": "cross-var sentry-cli releases files space@$npm_package_version delete --all"
  },
  "lint-staged": {
    "src/**/*.(js|ts|vue)": [
      "eslint --quiet"
    ]
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/mouse-wheel": "^2.4.2",
    "@chuangkit/chuangkit-design": "^2.1.0",
    "@rsbuild/plugin-css-minimizer": "^1.0.2",
    "@sentry/browser": "7.44.1",
    "@sentry/tracing": "7.44.1",
    "@sentry/vue": "7.44.1",
    "aes-js": "^3.1.2",
    "ant-design-vue": "4.2.6",
    "autoprefixer": "^10.4.20",
    "axios": "1.4.0",
    "cropperjs": "^1.5.12",
    "css-loader": "6.11.0",
    "dayjs": "^1.11.7",
    "echarts": "^5.1.2",
    "html-to-image": "^1.11.11",
    "js-pinyin": "^0.2.5",
    "jsbarcode": "^3.11.4",
    "localforage": "^1.10.0",
    "mutable-buffer": "^4.0.2",
    "postcss-loader": "7.3.4",
    "qrcode-parser": "2.1.3",
    "register-service-worker": "^1.7.2",
    "sortablejs": "1.14.0",
    "spark-md5": "^3.0.2",
    "tinymce": "6.7.3",
    "vue": "3.5.12",
    "vue-i18n": "9.4.1",
    "vue-loader": "^17.4.2",
    "vue-router": "4.4.5",
    "vuex": "4.1.0",
    "vuex-persistedstate": "4.0.0"
  },
  "devDependencies": {
    "@aaroon/workbox-rspack-plugin": "^0.3.2",
    "@cola-js/postcss-flexbox": "1.0.3",
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@rsbuild/core": "^1.0.14",
    "@rsbuild/plugin-eslint": "^1.0.4",
    "@rsbuild/plugin-stylus": "^1.0.3",
    "@rsbuild/plugin-vue": "^1.0.2",
    "@rsdoctor/rspack-plugin": "^0.4.8",
    "@types/aes-js": "3.1.1",
    "@types/google.accounts": "^0.0.15",
    "@types/sortablejs": "1.15.1",
    "@types/web-bluetooth": "0.0.16",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "@vue/cli-plugin-pwa": "5.0.8",
    "@vue/cli-plugin-typescript": "5.0.8",
    "cross-var": "1.1.0",
    "eslint": "8.36.0",
    "eslint-plugin-vue": "9.10.0",
    "husky": "8.0.3",
    "less": "4.1.3",
    "less-loader": "10.2.0",
    "lint-staged": "13.2.0",
    "raw-loader": "4.0.2",
    "stylus": "0.59.0",
    "stylus-loader": "7.1.3",
    "ts-loader": "^9.5.1",
    "typescript": "4.8.4",
    "vue-eslint-parser": "9.1.0"
  },
  "resolutions": {
    "fork-ts-checker-webpack-plugin": "6.5.2",
    "stylus-loader": "7.1.3"
  }
}
